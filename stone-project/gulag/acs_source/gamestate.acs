strict namespace{

int gamestate;

enum GAMESTATE
{
    GULAG_GAMESTATE_PLACEHOLDER, // zero place holder
    GULAG_WAITING,
    GULAG_COUNTDOWN,
    GULAG_INPROGRESS,
    GULAG_MATCHOVER
};

void setGameState(int what)
{
    LogDebug("setGameState() : \ckgamestate\c- has been set to \ck" + str(what) + "\c-");
    gamestate = what;
}

}